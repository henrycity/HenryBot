{
  "name": "henrybot",
  "version": "1.0.0",
  "description": "Finnish language detection bot.",
  "keywords": [
    "Slack",
    "bot"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tvtri96/henrybot"
  },
  "scripts": {
    "build": "babel bot.js -o server.js",
    "prestart": "npm run build",
    "start": "node server.js",
    "dev": "nodemon bot.js",
    "deploy": "npm run build && eb deploy"
  },
  "author": {
    "name": "Tri Tran",
    "email": "tvtri96@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@slack/client": "^3.8.1",
    "axios": "^0.16.2",
    "babel-runtime": "^6.26.0",
    "franc": "^3.1.1",
    "google-translate-api": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "nodemon": "^1.12.1",
    "xo": "^0.5.2"
  },
  "xo": {
    "space": 4,
    "rules": {
      "camelcase": [
        2,
        {
          "properties": "never"
        }
      ]
    }
  }
}
